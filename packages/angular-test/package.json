{
  "name": "angular-desktop-ui",
  "version": "0.2.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:build": "webpack-cli --mode development --progress --config webpack.electron.js",
    "electron:watch": "webpack-cli --mode development --watch --progress --config webpack.electron.js",
    "electron:start": "electron .",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac"
  },
  "bin": "dist/electron/electron.js",
  "main": "dist/electron/electron.js",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/forms": "^8.2.5",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "^8.2.5",
    "@marcj/estdlib": "^0.1.11",
    "core-js": "^2.5.4",
    "electron": "^6.0.1",
    "highlight.js": "^9.15.6",
    "npm-local-development": "^0.3.1",
    "rxjs": "~6.5.3",
    "showdown": "^1.9.0",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.0",
    "@angular/cli": "^8.2.0",
    "@angular/compiler-cli": "^8.2.5",
    "@angular/language-service": "^8.2.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@marcj/angular-desktop-ui": "^0.2.3",
    "@types/highlight.js": "^9.12.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/showdown": "^1.9.2",
    "babel-loader": "^8.0.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.3.0",
    "tslint": "~5.11.0",
    "typescript": "^3.4.5",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.1.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
