{
  "name": "@marcj/angular-desktop-ui",
  "version": "0.3.25",
  "description": "library offering you desktop UI widgets in Angular 7+",
  "keywords": [
    "Desktop",
    "UI",
    "MacOs"
  ],
  "bin": {
    "dui-create-font": "./bin/create-font.js"
  },
  "repository": "https://github.com/marcj/angular-desktop-ui",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "scripts": {
    "emoji": "ts-node emoji-map.ts",
    "font": "node bin/create-font.js",
    "build": "webpack && node bin/create-font.js && npm run typedoc",
    "prepublishOnly": "webpack && node bin/create-font.js",
    "docs": "typedoc --json src/assets/docs.json --mode modules src"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@marcj/estdlib": "^0.1.17",
    "@marcj/estdlib-rxjs": "^0.1.17",
    "@types/hammerjs": "^2.0.36",
    "hammerjs": "^2.0.8",
    "path": "^0.12.7",
    "tslib": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular-devkit/schematics": "^10.2.0",
    "@angular/animations": "^10.2.2",
    "@angular/cdk": "^9.0.0",
    "@angular/cli": "^10.2.0",
    "@angular/common": "^10.2.2",
    "@angular/compiler": "^10.2.2",
    "@angular/compiler-cli": "^10.2.2",
    "@angular/core": "^10.2.2",
    "@angular/forms": "^10.2.2",
    "@angular/platform-browser": "^10.2.2",
    "@angular/platform-browser-dynamic": "^10.2.2",
    "@angular/router": "^10.2.2",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.12.26",
    "awesome-typescript-loader": "^5.2.1",
    "fs-extra": "^8.1.0",
    "ng-packagr": "^10.1.0",
    "rxjs": "~6.5.3",
    "svg2ttf": "^4.2.0",
    "svgicons2svgfont": "^9.0.4",
    "transformation-matrix-js": "2.7.1",
    "ttf2woff": "^2.0.1",
    "typedoc": "^0.16.7",
    "typescript": "^4.0.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
